{
  "name": "webpack-boilerplate-react",
  "version": "0.1.0",
  "description": "A webpack boilerplate to use for react projects. Includes babel, eslint, prettier, jest and more.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.config.js --env env=dev",
    "build": "webpack --config config/webpack.config.js --env env=prod"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Mohammed Shibily",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.2",
    "webpack": "^5.58.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
